
<link rel="stylesheet" type="text/css" href="/css/style.css">

<div id="registration-container">
    <div class="rooms-content">
        <div class="rooms-container">
        <h2 class="place">This are different places where the visitor can stay</h2>
        
            

<div class="rooms-grid">
    {{#each roomsList}}
    <a class="room-link" href="">
                <div class="room">
                    <div class="box-img">
                        <img class="room-img" src="/public/images/{{newRoom.img}}" alt="Teste">
                    </div>
                    <h2 class="room-title">{{this.title}}</h2>
                    <h2 class="room-title">{{this.description}}</h2>
                    <h2 class="room-price">{{this.location}}</h2>
                    <h2 class="room-title">{{this.price}}/Night: $</h2>
                    <img class="room-title" src="/images/{{this.roomImage}}">
                    <input type="submit" value="Book Room" class="search" id="search"> 
                </div>
                {{else}}
                Empty Rooms
            </a>
            {{/each}}
</div>


